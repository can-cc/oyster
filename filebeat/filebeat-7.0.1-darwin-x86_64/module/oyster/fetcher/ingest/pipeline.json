{
  "description": "Pipeline for parsing oyster fetcher logs",
  "processors": [
    {
      "grok": {
          "field": "message",
          "patterns": [
              "%{GREEDYDATA:oyster.fetcher.message}"
          ]
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
